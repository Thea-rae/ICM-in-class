//ICM day one playing with a ball while going over basic shapes
int ball = 30;
void setup (){
  size (500,500);
  background(235);
  fill(90,40,60);
  strokeWeight(3);
  line(0,0,499,499);
  noStroke();
  rect(50,50,60,60);
  noFill();
  stroke(0);
  strokeWeight(3);
  ellipse(50,50,ball,ball);
  
}
void draw(){
  arc(
  smooth();
  strokeWeight(3);
  fill(120,10);
  point(250,250);
  noStroke();
  ellipse(mouseX,mouseY,ball, ball);
  fill(30,150,70,5);
  smooth();
  stroke(95);
  quad(100,120,90,140,mouseX,160,200,250);
}

